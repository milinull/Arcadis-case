{
  "name": "arcadis-case",
  "version": "1.0.0",
  "description": "Dashboard minimalista Arcadis Case desenvolvido em React",
  "main": "index.jsx",
  "author": "Raphael",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/arcadis-case.scss src/assets/css/arcadis-case.css",
    "minify:scss": "sass src/assets/scss/arcadis-case.scss src/assets/css/arcadis-case.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "ajv": "^8.16.0",
    "bootstrap": "4.6.2",
    "chart.js": "2.9.4",
    "classnames": "2.3.2",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": "18.2.0",
    "react-chartjs-2": "2.11.2",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.1",
    "react-scripts": "5.0.1",
    "reactstrap": "8.10.0",
    "sass": "1.69.5"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "cross-env": "^7.0.3"
  }
}
